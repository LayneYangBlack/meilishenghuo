<script id="scriptDiv" type="text/html">
    <% for (var i = 0; i < list.length; i ++) { %>
    <div class="item">
        <div class="title">
            <span class="type" style="font-size: 16px;"><%=list[i].title%></span>
            &nbsp&nbsp&nbsp<%=list[i].time%>
        </div>
        <div class="info_2">
            <span class="tagButton2">+<%=list[i].score%></span>

        </div>
        <div style="clear: both"></div>
        <div class="info">
            <%=list[i].desc%>
        </div>
    </div>

    </div>
    <% } %>
</script>


