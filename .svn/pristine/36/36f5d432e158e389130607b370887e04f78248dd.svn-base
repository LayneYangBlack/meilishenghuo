	/*position: fixed;*/
	background-color: #fff;
	height: 30px;
	border-bottom: 1px solid rgba(0,0,0,.1);
	/*z-index: 100;*/
	display: none;
}
			
.search {
	margin-left: 5px;
	/*padding-right: 42px;*/
	/*position: absolute;*/
}
		
.keyWord {
	width: 75%;
	-webkit-appearance: none;
	padding: 5px;
	margin-bottom: 15px;
	/*line-height: 21px;*/
	-webkit-user-select: text;
	background-color: #fff;
	border: 1px solid rgba(0, 0, 0, .2);
	border-radius: 3px;
	outline: none;
}
			
.searchBtn{
	text-align: center;
